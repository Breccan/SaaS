<div class="subscriptions">
  <table>
    <tr>
      <th> Name </th>
      <th> Cost </th>
      <th> Period </th>
    </tr>
    <% @subscriptions.each do |subscription| %>
      <tr>
        <td> <%= subscription.name %> </td>
        <td> <%= subscription.base_cost %> </td>
        <td> <%= subscription.period %> </td>
      </tr>
    <% end %>
  </table>

  <%= link_to "New", new_admin_subscription_path %> 
</div>

